{
  "Xsize": {
    "display_name": "X Size",
    "description": "用于设置图像宽度和高度的节点",
    "inputs": {
      "width": {
        "name": "宽度",
        "tooltip": "图像宽度（像素）"
      },
      "height": {
        "name": "高度",
        "tooltip": "图像高度（像素）"
      },
      "swap_dimensions": {
        "name": "交换宽高",
        "tooltip": "交换宽度和高度值"
      }
    },
    "outputs": {
      "0": {
        "name": "宽度",
        "tooltip": "输出宽度值"
      },
      "1": {
        "name": "高度",
        "tooltip": "输出高度值"
      }
    }
  },
  "Ximagesave": {
    "display_name": "X Image Save",
    "description": "支持自定义路径和文件名标识符的图像保存节点",
    "inputs": {
      "images": {
        "name": "图像",
        "tooltip": "要保存的图像"
      },
      "filename_prefix": {
        "name": "文件名前缀",
        "tooltip": "文件名前缀，支持标识符如 %date%, %time%, %seed% 等"
      },
      "use_custom_path": {
        "name": "使用自定义文件夹",
        "tooltip": "是否使用自定义文件夹"
      },
      "folder": {
        "name": "自定义文件夹名称",
        "tooltip": "自定义保存文件夹，支持同时创建子文件夹，如：人物/女性"
      }
    }
  }
